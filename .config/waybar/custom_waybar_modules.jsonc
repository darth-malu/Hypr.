        "2": "Ôâ© ",
        "8": "ÔÑõ ",
        "3": "ÔÑ† ",
        "10": "ÔÜº ",
        "1": "Ôãú ",
        "5": "ÓïÜ ",
        "4": "ÓïÖ ",
        "7": "ÓïÖ ",
        "9": "Ôïå ",
        "6": "Ôúî ",
        "urgent": "Ôûπ ",
        "focused": "ÔÑò",
        "empty": "X",
        "persistent": "Ôúî"
        //"default": "ÔÑë",
        //"active": "ÔÑò",
        //"default": "+"
        },
    "persistent-workspaces": {"*" :9},
    "show-special": true,
