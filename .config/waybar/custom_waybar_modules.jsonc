"wlr/taskbar": {
    "format": "{icon}",
    "icon-size": 16,
    "spacing": 3,
    //"active-first": true,
    "icon-theme": "Humanity-Dark",
    "tooltip-format": "{title}",
    "on-click": "activate",
    "on-click-right": "close",
    //"on-click-right": "minimize-raise",
    "ignore-list": [
       "Easy Effects"
    ],
    "app_ids-mapping": {
      "firefoxdeveloperedition": "firefox-developer-edition"
    },
    "rewrite": {
        "Firefox Web Browser": "Firefox",
