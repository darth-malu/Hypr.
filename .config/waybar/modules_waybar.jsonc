        "format-paused": "{status_icon}  {dynamic}",
        "format-playing": "{player_icon}  {dynamic}",
        "on-click-middle":"~/.darth/scripts/player_focus.sh", 
        //"on-scroll-down": "playerctl -p spotify,lollypop volume 0.1-",
        "on-scroll-down": "~/.darth/scripts/mpris_volume.sh -",
        "on-scroll-up": "~/.darth/scripts/mpris_volume.sh +",
        "tooltip":false,
        //"format-playing-len": 8playerctl -p spotify,lollypop volume 0.8+,
        "ellipsis": "...",
        //"interval": 3,
	    "player-icons": {
		    "default": "Ôéµ",
            "Lollypop": "Ôéµ ",
            //"spotify":"ÔÜº ",
            "spotify":"Û∞ìá",
            //"mpd":"‚ô™",
            "mpd":"Ó∞õ ",
		    "mpv": "üéµ"
	        },
	    "status-icons": {
		    "paused": "‚è∏",
            "playing": "ÔÖÑ",
            "stopped": "ÔÅç"
	        },
	    //"ignored-players": ["firefox", "smplayer"],
        "dynamic-importance-order": ["title", "artist", "album", "position", "length"],
        //"dynamic-len": 24,
        //"dynamic-len": 40,
        "dynamic-len": 30,
        //"dynamic-separator": "-",
        "dynamic-order":["title", "artist", "album"]
    },

"wlr/taskbar": {
