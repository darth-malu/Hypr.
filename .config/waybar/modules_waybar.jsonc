{
    "hyprland/workspaces": {
    /*"format": "{icon}",*/
    "format": "{name}",
    "on-click": "activate",
    "on-click-right": "close",
    "format-icons": {
        "2": "Ôâ© ",
        "8": "ÔÑõ ",
        "3": "ÔÑ† ",
        "10": "ÔÜº ",
        "1": "Ôãú ",
        "5": "ÓïÜ ",
        "4": "ÓïÖ ",
        "7": "ÓïÖ ",
        "9": "Ôïå ",
        "6": "Ôúî ",
        "urgent": "Ôûπ ",
        "focused": "ÔÑò",
        "empty": "X",
        "persistent": "Ôúî"
        //"default": "ÔÑë",
        //"active": "ÔÑò",
        //"default": "+"
        },
    "persistent-workspaces": {"*" :9},
    //"persistent-workspaces": {"5": [],"6": [],"7": []},
    "show-special": false,
    "on-scroll-up": "hyprctl dispatch workspace e-1",
    "on-scroll-down": "hyprctl dispatch workspace e+1",
    "sort-by-number": true
    //"ignore-workspaces": {10}
