    "format": "‚úåÔ∏è {}",
    "max-length": 8,
    "tooltip": false
},


"idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
        //"activated": "ÔÅÆ",
        //"activated": " Ôû∂ ",
        "activated": " Ôû∂ ",
        //"deactivated": "ÔÅ∞"
        "deactivated": " Óïû "
    },
    "tooltip": false
},

"tray": {
    "icon-size": 11,
    "spacing": 8
},

"cpu": {
    "format": "{usage}%  Ôãõ ",
    "tooltip": false,
    "max-length": 6,
    "min-length": 6
},

"memory": {
    "format": "{}%  Ôî∏ "
},

"temperature": {
    // "thermal-zone": 2,
    "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input",
    "critical-threshold": 80,
    "format-critical": "{temperatureC}¬∞ {icon}",
    "interval": 8,
    "format": "{temperatureC} ¬∞",
    "format-icons": ["Ôù´ ", "Ôãâ", "Ôù© "],
    "tooltip": false
},

"network": {
    // "interface": "wlp2*", // (Optional) To force the use of this interface
