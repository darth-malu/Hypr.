    "hyprland/window": {
    //"format": " ÔÉö  {}",
    //"format": "\t{}",
    //"format": " {}",
    "format": "\t{}",
    "icon": false,
    "icon-size": 14,
    "max-length": 65,
    "rewrite": {
        "(.*) ‚Äî Mozilla Firefox": "\t>$1",
        "v (.*)": "\tÓöÆ$1",
        "(.*)~": "\tÔÑ° $1"
        //"(.*) - kitty": "Ôï∏ [$1]",
        //"(.*)~": "ÔÇÖ $1",
        //"(.*) - Spotify": "molog",
        //"(.*) - Lollypop": "molog"
        },
    "separate-outputs": true
    },


    "custom/gpu": {
    "exec": "~/.darth/scripts/temp.sh gpu_busy",
    "format": "{}%  ÔïÑ ",
    "return-type": "",
    "interval": 5,
    "min-length": 8,
    "max-length": 8,
    "tooltip": false
    },

    "custom/gpu_temp": 
    {
    "exec": "~/.darth/scripts/temp.sh gpu_temp",
    "interval": 10,
    "format": "{} ¬∞",
    "return-type": "",
    "min-length": 3,
    "max-length": 3,
    "tooltip": false
    },

    "custom/gpu_fans": 
    {
    "exec": "~/.darth/scripts/temp.sh gpu_fans",
    "interval": 10,
    "format": "{} rpm",
    "return-type": "",
    "min-length": 8,
    "max-length": 8,
    "tooltip": false
    },

    "custom/gpu_freq": 
    {
    "exec": "~/.darth/scripts/temp.sh gpu_freq",
    "interval": 20,
    "format": "{}",
    "return-type": "",
    //"min-length": 3,
    //"max-length": 3,
    "tooltip": false
    },

    "custom/weather": {
    //"exec": "~/.darth/scripts/get_weather.sh Nairobi+Kenya",
    "exec": "~/.darth/scripts/weather.sh",
    "return-type": "json",
    "format": "{}",
    "tooltip": false,
    "interval": 360,
    "cursor":true
    },

    "group/cpu_block": {
        "drawer": {"transition-duration": 400,"transition-left-to-right": true, "children-class": "drawer-child", "click-to-reveal": true},
        //"orientation": "horizontal",
        "orientation": "inherit",
        "cursor": true,
        "modules":[
            //"network",
            "cpu",
            "custom/cpu_freq"
            //"temperature"
        ]
    },

    "custom/cpu_freq": {
    "exec": "~/.darth/scripts/temp.sh cpu_freq",
    "format": "{} Ghz",
    "return-type": "",
    "interval": 5,
    "min-length": 8,
    "max-length": 8,
    "tooltip": false
    },

    "custom/nvme_temp": {
    "exec": "~/.darth/scripts/temp.sh cpu_freq",
    "format": "{} Ghz",
    "return-type": "",
    "interval": 10,
    "min-length": 8,
    "max-length": 8,
    "tooltip": false
    },


    "group/conglo": {
        "drawer": {"transition-duration": 500,"transition-left-to-right": true, "children-class": "drawer-child", "click-to-reveal": true},
        "orientation": "horizontal",
        "modules":[
            "custom/weather",
            "network"
            //"idle_inhibitor"
            //"temperature"
        ]
    },

    "group/media": {
        "orientation": "horizontal",
        "drawer": {"transition-duration": 400,"transition-left-to-right": true, "children-class": "drawer-child"},
        "modules":[
            //"network",
            "wireplumber",
            "mpris"
        ]
    },

    "keyboard-state": {
        //"format":"{name} {icon}",
        "numlock":false,
        "capslock":true,
        "format": { "numlock": "# {icon}",
                    "capslock": "Caps {icon}"},
        "format-icons": {
            "locked": "ÔàÖ",
            "unlocked": "ÔàÑ"
        }
    },

    "clock": {
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "timezone": "Africa/Nairobi",
        "format-alt": "{:%Y-%m-%d}",
        "min-length": 17,
        "max-length": 17,
        "format": "{:%a %d,  %I:%M %p }",
        "tooltip": true,
        "calendar": {
                    "mode"          : "month",
                    "mode-mon-col"  : 3,
                    "weeks-pos"     : "", //possible values: right:left
                    "on-scroll"     : 1,
                    "on-click-right": "mode",
                    "format": {
                              "months":     "<span color='#ffead3'><b>{}</b></span>",
                              "days":       "<span color='#ecc6d9'><b>{}</b></span>",
                              "weeks":      "<span color='#99ffdd'><b>W{}</b></span>",
                              "weekdays":   "<span color='#ffcc66'><b>{}</b></span>",
                              "today":      "<span color='#ff6699'><b><u>{}</u></b></span>"
                              }
                    },
        "actions":  {
                    "on-click-right": "mode",
                    "on-scroll-down": "shift_down"
                    }
    },


    "custom/wleave": {
        //"format": "ÔÄë",
        "format": "Û∞£á ",
        //"on-click": "wlogout",
        "tooltip": false
    },


    "custom/swap_workspace": {
        //"format": "ÔÄë",
        //Óïë  Óîµ ÔãúÔö≠Ôî¥Ôúû
        "format": "#",
        "on-click": "hyprctl dispatch focuscurrentorlast",
        "on-click-right": "hyprctl dispatch workspace 2",
        //"on-click-right": "$ (~/.config/waybar/launch_waybar.sh)",
        "tooltip": false
    },

     "custom/updates": {
        "format": "{} ÔÇ´ ",
        //"exec": "checkupdates --nocolor | wc -l && dunstify 'Yo, you have updates'",
        "exec": "checkupdates --nocolor | wc -l",
        "exec-if": "[[ $(checkupdates --nocolor | wc -l) != 0 ]]",
        "interval": 3600,
        "on-click": "kitty --hold -1 sh -c '~/.darth/scripts/waybar_updates.sh main'",
        "on-click-right": "kitty --hold -1 sh -c '~/.darth/scripts/waybar_updates.sh aur'",
        "signal": 8
    }, 


    "wireplumber": {
        //"format": "{volume} Óí∑ ",
        //"format": "{volume}%  Óí∑",
        "format": "{volume}\tÓí∑",
        "format-muted": "ÔÄ¶",
        "on-click": "killall pavucontrol || pavucontrol",
        "on-click-right": "wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle",
        "max-volume": 100,
        //"on-scroll-down": "~/.darth/scripts/volume_sink_ctrl.sh -",
        //"on-scroll-up": "~/.darth/scripts/volume_sink_ctrl.sh +",
