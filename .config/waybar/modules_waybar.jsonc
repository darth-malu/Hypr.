    "justify": "center"
    /*"align": 1*/
},

"pulseaudio/slider": {
    "min": 0,
    "max": 100,
    "orientation": "horizontal"
},

"load": {
    "interval": 10,
    "format": "ÔïÑ {load1}",
    "max-length": 10
}

}
